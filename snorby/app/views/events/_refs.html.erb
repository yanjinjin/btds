<div class='section-title plus'>参考</div>

<div class='references-table plus'>
	
	<table class='default' border='0' cellspacing='0' cellpadding='0'>
		<tr>
			<th class='first'>类型</th>
			<th class='last'>值</th>
		</tr>
    <tbody>
      <% @event.signature.sig_references.each do |ref| %>
        <tr>
          
          <td>
            <%= ref.reference.type %>
          </td>

          <td>
            <%= pretty_reference(ref.reference) %> 
          </td>

        </tr>
      <% end %> 
		</tbody>
	</table>

</div>

